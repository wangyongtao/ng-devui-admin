{
  "name": "ng-devui-admin",
  "version": "1.0.0",
  "license": "MIT",
  "description": "DevUI Admin based on Angular and DevUI components",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevCloudFE/ng-devui-admin"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 8001",
    "build": "ng build",
    "build:website": "ng build --configuration production --base-href /admin/ --deploy-url /admin/ --output-path dist/admin",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint": "tslint --fix -c ./tslint.json 'src/**/*{.ts}'",
    "prettier": "prettier --config ./.prettierrc --write \"src/app/pages/**/**/*.html\"",
    "stylelint": "stylelint \"src/app/pages/**/**/*.{scss,css}\" --config ./.stylelintrc.json --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.5",
    "@angular/cdk": "17.3.5",
    "@angular/common": "17.3.5",
    "@angular/compiler": "17.3.5",
    "@angular/core": "17.3.5",
    "@angular/forms": "17.3.5",
    "@angular/platform-browser": "17.3.5",
    "@angular/platform-browser-dynamic": "17.3.5",
    "@angular/router": "17.3.5",
    "@devui-design/icons": "^1.3.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "color": "^4.2.3",
    "echarts": "5.5.0",
    "ng-devui": "^17.0.0",
    "rxjs": "7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4",
    "gridstack": "^10.1.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular/cli": "^17.3.5",
    "@angular/compiler-cli": "^17.3.5",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^20.12.7",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "protractor": "~7.0.0",
    "shelljs": "^0.8.5",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.2.1",
    "ts-node": "~10.9.2",
    "tslint": "~6.1.0",
    "typescript": "~5.4.5"
  }
}